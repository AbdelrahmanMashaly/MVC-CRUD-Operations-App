@model UserViewModel
@{
    ViewData["Title"] = "Delete";
}
<h1>Delete</h1>


<form asp-action="DeleteConfirmed">
    <div asp-validation-summary="All"></div>
    <input asp-for="Id" class="form-control" hidden />
    <div class="form-group">
        <label class="col-form-label" asp-for="FName"></label>
        <input asp-for="FName" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="LName"></label>
        <input asp-for="LName" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="Email"></label>
        <input asp-for="Email" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="Phone"></label>
        <input asp-for="Phone" class="form-control" readonly />
    </div>
    <input type="submit" value="Delete" class="btn btn-primary" />
    <a asp-action="Index" class="btn btn-light">Back To list</a>
</form>
