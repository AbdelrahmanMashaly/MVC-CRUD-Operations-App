@model UserViewModel
@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>


<form asp-action="Edit">
    <div asp-validation-summary="All"></div>
    <div class="form-group">
    <label class="col-form-label" asp-for="FName"></label>
    <input asp-for="FName" class="form-control" />
</div>
    <div class="form-group">
        <label class="col-form-label" asp-for="LName"></label>
        <input asp-for="LName" class="form-control" />
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="Phone"></label>
        <input asp-for="Phone" class="form-control" />
    </div>
    <input type="submit" value="Submit" class="btn btn-primary" />
    <a asp-action="Index" class="btn btn-light">Back To list</a>
</form>

@section ValidateClientSide {
    <partial name="_ValidationScriptsPartial" />
}
